syntax = "v1"

type BaseTree {
    id int `json:"id"`
}

type Tree {
    base BaseTree `json:"base"`
}

type User {
    username string `json:"username"`
}

type EmbededError3 {
    message string `json:"message"`
}

type Error3 {
    pointerType *EmbededError3 `json:"pointerType"`
}

@server (
    prefix: /api/v1
    group: err1
)
service test {
    @handler Error1
    post /test/error1 () returns ([]Tree)
}

@server (
    prefix: /api/v1
    group: err3
)
service test {
    @handler Error3
    post /test/error3 () returns (Error3)
}